{
  "parser": "eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": "always",
    "eol-last": 1,
    "no-multiple-empty-lines": 1,
    "no-multi-spaces": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 0,
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ],
    "strict": [ 2, "global" ]
  }
}
